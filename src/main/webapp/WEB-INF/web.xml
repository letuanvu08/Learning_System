<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>attend</servlet-name>
        <servlet-class>com.Learning.Student.Attend</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>attend</servlet-name>
        <url-pattern>/student/attend</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>updateSubject</servlet-name>
        <servlet-class>com.Learning.faculty.UpdateSubject</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>updateSubject</servlet-name>
        <url-pattern>/faculty/updateSubject</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>updateLecturerClass</servlet-name>
        <servlet-class>com.Learning.faculty.updateLecturerClassControler</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>updateLecturerClass</servlet-name>
        <url-pattern>/faculty/updateLecturerClass</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>subjectattend</servlet-name>
        <servlet-class>com.Learning.Student.SubjectAttendController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>subjectattend</servlet-name>
        <url-pattern>/student/subjectattend</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>ListSubclassAttendedControler</servlet-name>
        <servlet-class>com.Learning.Student.ListSubclassAttendedControler</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ListSubclassAttendedControler</servlet-name>
        <url-pattern>/student/ListSubclassAttended</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>NoCreditStudent</servlet-name>
        <servlet-class>com.Learning.Student.NoCreditStudentControler</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NoCreditStudent</servlet-name>
        <url-pattern>/student/NoCreditStudent</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>com.Learning.profile.Logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>changepassword</servlet-name>
        <servlet-class>com.Learning.profile.ChangePassword</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>changepassword</servlet-name>
        <url-pattern>/student/changepassword</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>profileservlet</servlet-name>
        <servlet-class>com.Learning.profile.ProfileController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>profileservlet</servlet-name>
        <url-pattern>/lecturer/profileservlet</url-pattern>
        <url-pattern>/student/profileservlet</url-pattern>
        <url-pattern>/faculty/profileservlet</url-pattern>
        <url-pattern>/aaoemployee/profileservlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>updateclassservlet</servlet-name>
        <servlet-class>com.Learning.aaoemployee.updateclassController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>updateclassservlet</servlet-name>
        <url-pattern>/aaoemployee/updateclassController</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>subclassofmembercontroller</servlet-name>
        <servlet-class>com.Learning.aaoemployee.subclassOfmemberController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>subclassofmembercontroller</servlet-name>
        <url-pattern>/aaoemployee/subclassOfmemberController</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>detailfacultyController</servlet-name>
        <servlet-class>com.Learning.aaoemployee.detailfacultyController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>detailfacultyController</servlet-name>
        <url-pattern>/aaoemployee/detailfaculty</url-pattern>
    </servlet-mapping>

    <!--    ################################################ -->
    <servlet>
        <servlet-name>subclassofmember</servlet-name>
        <jsp-file>/pages/aaoemployee/SubclassOfMember.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>subclassofmember</servlet-name>
        <url-pattern>/aaoemployee/subclassofmember</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>updateclass</servlet-name>
        <jsp-file>/pages/aaoemployee/updateclass.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>updateclass</servlet-name>
        <url-pattern>/aaoemployee/updateclass</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>subclassdetail</servlet-name>
        <jsp-file>/pages/aaoemployee/subclassdetail.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>subclassdetail</servlet-name>
        <url-pattern>/aaoemployee/subclassdetail</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>profile</servlet-name>
        <jsp-file>/pages/Account/view_profile.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>profile</servlet-name>
        <url-pattern>/aaoemployee/profile</url-pattern>
        <url-pattern>/faculty/profile</url-pattern>
        <url-pattern>/lecturer/profile</url-pattern>
        <url-pattern>/student/profile</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>mainlecturer</servlet-name>
        <jsp-file>/pages/lecturer/main.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>mainlecturer</servlet-name>
        <url-pattern>/lecturer/main</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>mainfaculty</servlet-name>
        <jsp-file>/pages/faculty/main.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>mainfaculty</servlet-name>
        <url-pattern>/faculty/main</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>maintraining</servlet-name>
        <jsp-file>/pages/aaoemployee/main.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>maintraining</servlet-name>
        <url-pattern>/aaoemployee/main</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>login</servlet-name>
        <jsp-file>/pages/login.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>welcome</servlet-name>
        <jsp-file>/welcome.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>welcome</servlet-name>
        <url-pattern>/welcome</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>mainStudent</servlet-name>
        <jsp-file>/pages/student/main.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>mainStudent</servlet-name>
        <url-pattern>/student/main</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>index</servlet-name>
        <jsp-file>/NewBiz/index.html</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>index</servlet-name>
        <url-pattern>/student/index</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>home</servlet-name>
        <servlet-class>com.Learning.Login.MainPage</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>home</servlet-name>
        <url-pattern></url-pattern>
    </servlet-mapping>

    <filter>
        <filter-name>loginFilter</filter-name>
        <filter-class>com.Learning.Filter.LoginFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>loginFilter</filter-name>
        <url-pattern>/student/*</url-pattern>
        <url-pattern>/aaoemployee/*</url-pattern>
        <url-pattern>/lecturer/*</url-pattern>
        <url-pattern>/faculty/*</url-pattern>
    </filter-mapping>

</web-app>